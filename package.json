{
	"name": "youtubeshorts",
	"version": "0.0.0",
	"scripts": {
		"build": "npx ts-node build.ts",
		"lint": "npx eslint --ext .ts src/",
		"lint:tests": "npx eslint --ext .ts spec/",
		"start": "npm run build && node -r module-alias/register ./dist ",
		"dev": "nodemon",
		"test": "nodemon --config ./spec/nodemon.json",
		"test:no-reloading": "npx ts-node --files -r tsconfig-paths/register ./spec"
	},
	"nodemonConfig": {
		"watch": [
			"src"
		],
		"ext": "ts, html",
		"ignore": [
			"src/public"
		],
		"exec": "./node_modules/.bin/ts-node --files -r tsconfig-paths/register ./src"
	},
	"_moduleAliases": {
		"@src": "dist"
	},
	"engines": {
		"node": ">=8.10.0"
	},
	"dependencies": {
		"@ffmpeg-installer/ffmpeg": "^1.1.0",
		"@types/axios": "^0.14.0",
		"@types/bcrypt": "^5.0.0",
		"@types/cookie-parser": "^1.4.3",
		"@types/find": "^0.2.1",
		"@types/fluent-ffmpeg": "^2.1.20",
		"@types/fs-extra": "^11.0.1",
		"@types/jasmine": "^4.3.1",
		"@types/jsonfile": "^6.1.1",
		"@types/jsonwebtoken": "^9.0.1",
		"@types/lodash": "^4.14.191",
		"@types/node": "^18.14.1",
		"@types/request": "^2.48.8",
		"@types/supertest": "^2.0.12",
		"@typescript-eslint/eslint-plugin": "^5.52.0",
		"@typescript-eslint/parser": "^5.52.0",
		"assert": "^2.0.0",
		"axios": "^0.21.1",
		"bcrypt": "^5.1.0",
		"body-parser": "^1.20.2",
		"child_process": "^1.0.2",
		"cookie-parser": "^1.4.6",
		"dotenv": "^16.0.3",
		"express": "^4.18.2",
		"express-async-errors": "^3.1.1",
		"fluent-ffmpeg": "^2.1.2",
		"fs": "^0.0.1-security",
		"fs-extra": "^11.1.0",
		"google-auth-library": "^8.7.0",
		"googleapis": "^111.0.0",
		"helmet": "^6.0.1",
		"inserturlparams": "^1.0.1",
		"jet-logger": "^1.3.1",
		"jet-validator": "^1.0.4",
		"jsonfile": "^6.1.0",
		"jsonwebtoken": "^9.0.0",
		"lodash": "^4.17.21",
		"module-alias": "^2.2.2",
		"mongoose": "^6.9.1",
		"morgan": "^1.10.0",
		"readline": "^1.3.0",
		"snoowrap": "^1.23.0",
		"ts-command-line-args": "^2.4.2"
	},
	"devDependencies": {
		"@types/bcrypt": "^5.0.0",
		"@types/cookie-parser": "^1.4.3",
		"@types/express": "^4.17.17",
		"@types/find": "^0.2.1",
		"@types/fluent-ffmpeg": "^2.1.20",
		"@types/jasmine": "^4.3.1",
		"@types/jsonfile": "^6.1.1",
		"@types/jsonwebtoken": "^9.0.1",
		"@types/lodash": "^4.14.191",
		"@types/morgan": "^1.9.4",
		"@types/node": "^18.14.1",
		"@types/request": "^2.48.8",
		"@types/supertest": "^2.0.12",
		"@typescript-eslint/eslint-plugin": "^5.52.0",
		"@typescript-eslint/parser": "^5.52.0",
		"eslint": "^8.34.0",
		"eslint-plugin-node": "^11.1.0",
		"find": "^0.3.0",
		"jasmine": "^4.5.0",
		"nodemon": "^2.0.20",
		"supertest": "^6.3.3",
		"ts-node": "^10.9.1",
		"tsconfig-paths": "^4.1.2",
		"typescript": "^4.9.5"
	}
}
